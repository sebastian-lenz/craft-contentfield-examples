label: Text
icon: material:format_align_left
preview: >
  {{ body }}
fields:
  body: redactor
---
<div{{ editAttributes }}>
  <h4>Member #{{ loop.index }}</h4>

  {% if info is defined %}
    <div class="alert alert-secondary" role="alert">
      Passed info: <strong>{{ info }}</strong>
    </div>
  {% endif %}

  {{ body }}
</div>
